  "description": "A vile plugin for rubycritic",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "license": "MPL-2.0",
  "scripts": {
    "babel": "babel",
    "coffee": "coffee",
    "compile": "node bin/compile",
    "compile-dev": "node bin/compile-dev",

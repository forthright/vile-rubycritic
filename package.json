    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "^1.12.3",
    "istanbul": "^0.4.5",
    "mimus": "^0.2.3",
    "mocha": "^3.2.0",
    "npm-check-updates": ">=2.10.2",
    "release-it": "^2.5.4",
